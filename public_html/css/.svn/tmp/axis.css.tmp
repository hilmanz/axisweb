/* ============ STATUS USER =================*/
.status{
	width: 220px;
	height: auto;
	line-height: 20px;
	padding:0;
	left: 50%;
	margin-left: -110px;
	font-weight: 400;	
	color: #a02a8d;
	text-shadow: 1px 1px 1px rgba(0, 0, 0, 0.1);
	text-align: center;
	border-bottom: 4px solid #fff;
	background:#fff;
	text-indent: 0px;
	border-radius:0 0 5px 5px;
	position: absolute;
	bottom:230px;
	box-shadow: 1px 1px 2px rgba(0,0,0,0.1);
	-webkit-transition: all 0.3s ease-in-out;
	-moz-transition: all 0.3s ease-in-out;
	-o-transition: all 0.3s ease-in-out;
	-ms-transition: all 0.3s ease-in-out;
	transition: all 0.3s ease-in-out;
	z-index:100;
}
.status:before,
.status:after{
	content: '';
	position: absolute;
	bottom: -15px;
	left: 50%;
	margin-left: -9px;
	width: 0;
	height: 0;
	border-left: 10px solid transparent;
	border-right: 10px solid transparent;
	border-top: 10px solid rgba(0,0,0,0.1);
}
.status:after{
	bottom: -14px;
	margin-left: -10px;
	border-top: 10px solid #fff;
}
.status .text{
	background: #e7dcc9; /* Old browsers */
	background: -moz-linear-gradient(top, #e7dcc9 0%, #ffffff 33%, #ffffff 100%); /* FF3.6+ */
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#e7dcc9), color-stop(33%,#ffffff), color-stop(100%,#ffffff)); /* Chrome,Safari4+ */
	background: -webkit-linear-gradient(top, #e7dcc9 0%,#ffffff 33%,#ffffff 100%); /* Chrome10+,Safari5.1+ */
	background: -o-linear-gradient(top, #e7dcc9 0%,#ffffff 33%,#ffffff 100%); /* Opera 11.10+ */
	background: -ms-linear-gradient(top, #e7dcc9 0%,#ffffff 33%,#ffffff 100%); /* IE10+ */
	background: linear-gradient(to bottom, #e7dcc9 0%,#ffffff 33%,#ffffff 100%); /* W3C */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#e7dcc9', endColorstr='#ffffff',GradientType=0 );
	padding:5px;
	display:block;
	clear:both;
}
.status .nickname{
	float:left;
	margin:0 0 0 5px;
	width:80px;
	display:block;
	text-align:left;
}
.status:hover{
	bottom:210px;
}
